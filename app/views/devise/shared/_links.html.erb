<% if controller_name != 'sessions' %> 
  <%= link_to t('.sign_in', default: 'Sign in'), new_session_path(resource_name), class: "link-primary" %><br />
<% end %>

<% if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Create an Account", new_registration_path(resource_name), class: 'link-primary' %><br />
<% end %>